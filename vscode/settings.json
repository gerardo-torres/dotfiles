{
    "workbench.colorCustomizations": {
        "statusBar.background": "#537666",
        // "statusBar.background": "#643d3d",
        "editor.background": "#1b1b1b",
        "editor.lineHighlightBackground": "#333435",
        "sideBar.background": "#1b1b1b",
        "editorLineNumber.foreground": "#6d786b",
        "list.activeSelectionBackground":"#404b4e",
        "tab.inactiveBackground": "#414141",
        "button.hoverBackground": "#414141",
        "input.background": "#414141",
        "list.inactiveSelectionBackground": "#414141",
    },
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.colorTheme": "Monokai",
    "editor.minimap.enabled": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "explorer.confirmDelete": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "window.zoomLevel": -1,
    "window.menuBarVisibility": "toggle",
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "browser",
    "editor.fontSize": 15,
    "telemetry.enableTelemetry": false,
    "explorer.confirmDragAndDrop": false,
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "black",
    "liveServer.settings.donotVerifyTags": true,
    "python.pythonPath": "/usr/bin/python3",
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.startupEditor": "welcomePage",
    "window.enableMenuBarMnemonics": false,
    "vim.useCtrlKeys": false,
}